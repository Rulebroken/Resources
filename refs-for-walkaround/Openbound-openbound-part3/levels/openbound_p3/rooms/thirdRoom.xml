<rooms>
  <room name='thirdRoom' walkableMap='thirdRoomMap' mapScale='2'>
  	<triggers>

  		<trigger operator="OR">
  			<args>inBox2,kanaya,2991,1911,2841,1812</args>
        <args>inBox2,damara,2991,1911,2841,1812</args>
  			<action command='changeRoomRemote'>rooms/secondRoom.xml,secondRoom,1326,380</action>
  		</trigger>

      <trigger operator='OR'>
        <args>inBox2,rose,2991,1911,2841,1812</args>
        <args>inBox2,dave,2991,1911,2841,1812</args>
