          <action command='addSprite'>
            <args>karkat,sixthRoom</args>
          </action>
        </action>
      </trigger>

  		<trigger>
  			<args>inBox2,kanaya,345,831,225,609</args>
        <action command='goBack' noDelay='true'>
          <args>kanaya</args>
          <action command='fadeOut'>
            <action command='unfollow'>
              <args>meenah</args>
              <action command='changeChar'>
                <args>meenah</args>
      			    <action command='changeRoomRemote'>
                  <args>rooms/fourthRoom.xml,fourthRoom,1896,399</args>
                  <action command='playAnimation'>
                    <args>araneaStand,aranea</args>
                    <action command='playSong'>elevatorStuck,10.895</action>
                  </action>
                </action>
              </action>
            </action>
          </action>
