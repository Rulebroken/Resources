	<character class='meenah' name='meenah' sx='-50' sy='-110' sWidth='100' sHeight='130' width='30' height='15' sheet='meenahSheet' state='walkFront' facing='Front'>
		
	</character>
	
	<action class='beMeenah' name='Be Meenah.' command='changeChar'>
